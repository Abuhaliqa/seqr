export PS1="\h:\w]$ "

export SHELL=/bin/bash

export LS_OPTIONS='--color=auto'
eval "`dircolors`"
alias ll="ls -al"
alias less='less -m -g -i--underline-special --SILENT'
alias more='less'

export TERM=xterm

# directories

alias code='cd /seqr'
alias settings='cd /seqr_settings'

# databases

alias mongo='mongo --host ${MONGO_SERVICE_HOSTNAME}'

resize

alias gs='gsutil'


