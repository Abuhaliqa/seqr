{
  "name": "seqr",
  "version": "0.2.0",
  "description": "seqr",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "onchange -i -e 'dist' '.' -- node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macarthur-lab/seqr.git"
  },
  "keywords": [
    "seqr"
  ],
  "author": "",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/macarthur-lab/seqr/issues"
  },
  "homepage": "https://github.com/macarthur-lab/seqr#readme",
  "babel": {
    "plugins": [
      "babel-plugin-transform-decorators-legacy",
      "babel-plugin-transform-inline-environment-variables",
      "babel-plugin-transform-object-rest-spread",
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".css"
          ],
          "alias": {
            "shared": "./shared/",
            "pages": "./pages/"
          }
        }
      ]
    ],
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "env",
      "stage-2"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/scripts"
    ]
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "http-proxy-middleware": "^0.17.3",
    "jest": "^19.0.2",
    "postcss-font-magician": "^1.6.0",
    "postcss-loader": "^1.3.3",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.16.1",
    "why-did-you-update": "0.0.8"
  },
  "dependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^1.1.3",
    "datatables.net": "^2.1.1",
    "detect-port": "^1.0.7",
    "dotenv": "^4.0.0",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "filesize": "^3.3.0",
    "fs-extra": "^2.1.2",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "^2.10.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "onchange": "^3.2.1",
    "path-exists": "^3.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-font-magician": "^1.6.0",
    "purifycss-webpack-plugin": "^2.0.3",
    "random-material-color": "^1.0.3",
    "react": "^15.4.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dev-utils": "^0.5.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-render-visualizer-decorator": "^0.4.1",
    "react-router": "^4.1.1",
    "react-tinymce": "^0.5.1",
    "react-virtualized": "^9.7.3",
    "recursive-readdir": "^2.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0",
    "semantic-ui-react": "^0.67.2",
    "timeago.js": "^3.0.1",
    "timeout-as-promise": "^1.0.0",
    "webpack": "^2.3.3",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-validator": "^3.0.0",
    "whatwg-fetch": "^2.0.1",
    "why-did-you-update": "0.0.8"
  }
}
